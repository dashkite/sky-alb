AWSTemplateFormatVersion: "2010-09-09"
Description: Dispatcher {{name}} {{env}}
Resources:

  {{> alb/core.yaml }}

  {{> alb/lambda.yaml }}

  {{> alb/edge.yaml }}

  {{#if mixins}}
  {{#each mixins}}
  
  {{#if (equal type "graphite") }}
  {{> mixins/graphite.yaml }}
  {{/if}}

  {{#if (equal type "s3") }}
  {{> mixins/s3.yaml }}
  {{/if}}

  {{/each}}
  {{/if}}
